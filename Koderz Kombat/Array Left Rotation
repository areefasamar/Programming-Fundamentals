#include<stdio.h>
int main(void)
{
	int n, d, i, k=0;
	
	scanf("%d %d",&n, &d);
	
	int a[n],b[n];
	
	for ( i=0 ; i<n ; i++)
	{
		scanf("%d",&a[i]);
	}
	
	for ( i=d ; i<n ; i++ )
	{
		b[k]= a[i];
		k+=1;
	}
	
	for ( i=0 ; i<d ; i++)
	{
		b[k]=a[i];
		k+=1;
	}
	
	for ( i=0 ; i<n ; i++)
	{
		printf("%d ",b[i]);
	}
	return 0;
}
